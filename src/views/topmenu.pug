nav.navbar.navbar-expand-lg.navbar-light.custom-header-bgcolor(class=data.class)
    div.container.col-12.col-lg-10
        if (data.brandlogo != '') && (data.brandlogo != undefined)
            a.navbar-brand(href=data.brandlogolink)
                img(src=data.brandlogo)
        button.navbar-toggler(type="button", data-toggle="collapse", data-target="#wbTopMenu", aria-controls="bs-navbarNav", aria-expanded="false", aria-label="Toggle navigation")
            span.navbar-toggler-icon
        div.navMenu.collapse.justify-content-end.navbar-collapse(id="wbTopMenu")
            ul.navbar-nav.top-menu
                each menuitem in data.menu
                    li.nav-item.top-menu-item
                        if data.html
                            a.nav-link(href=`${menuitem.link}.html`) #{menuitem.title}
                        else
                            a.nav-link(href=menuitem.link) #{menuitem.title}
